{
  "framework": null,
  "rewrites": [
    { "source": "/api/stream", "destination": "/api/stream.json" },
    { "source": "/live/:path*", "destination": "/live.html" },
    { "source": "/vip", "destination": "/vip.html" },
    { "source": "/:path*", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/api/stream.json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" }
      ]
    }
  ]
}

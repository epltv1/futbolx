{
  "framework": null,
  "rewrites": [
    { "source": "/api/stream", "destination": "/api/stream.json" },
    { "source": "/live/:path*", "destination": "/live.html" },
    { "source": "/vip", "destination": "/vip.html" },
    { "source": "/:path*", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/api/stream.json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" }
      ]
    }
  ],
  "builds": [
    {
      "src": "api/stream-token.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/stream.json",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    }
  ]
}
